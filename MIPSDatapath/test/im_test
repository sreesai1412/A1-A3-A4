#! /usr/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision + 0;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_0x1beaf20 .scope module, "im_test" "im_test" 2 3;
 .timescale 0 0;
v0x1bfdf60_0 .var "addr", 31 0;
v0x1bfe030_0 .var "clk", 0 0;
v0x1bfe0e0_0 .net "data", 31 0, L_0x1bfe280; 1 drivers
S_0x1beb010 .scope module, "m" "im" 2 11, 3 1, S_0x1beaf20;
 .timescale 0 0;
L_0x1bfe280 .functor BUFZ 32, L_0x1bfe190, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>, C4<00000000000000000000000000000000>;
v0x1bc8130_0 .net *"_s0", 31 0, L_0x1bfe190; 1 drivers
v0x1bfdcf0_0 .net "addr", 31 0, v0x1bfdf60_0; 1 drivers
v0x1bfdd90_0 .net "clk", 0 0, v0x1bfe030_0; 1 drivers
v0x1bfde30_0 .alias "data", 31 0, v0x1bfe0e0_0;
v0x1bfdee0 .array "mem", 255 0, 31 0;
L_0x1bfe190 .array/port v0x1bfdee0, v0x1bfdf60_0;
    .scope S_0x1beb010;
T_0 ;
    %vpi_call 3 8 "$readmemh", "../lib/initRegisters.txt", v0x1bfdee0, 1'sb0, 6'sb011110;
    %vpi_call 3 9 "$readmemh", "../program/test.txt", v0x1bfdee0, 6'sb011111, 9'sb011100000;
    %vpi_call 3 10 "$readmemh", "../lib/regVals.txt", v0x1bfdee0, 9'sb011100001, 9'sb011111111;
    %end;
    .thread T_0;
    .scope S_0x1beaf20;
T_1 ;
    %delay 1, 0;
    %load/v 8, v0x1bfe030_0, 1;
    %inv 8, 1;
    %set/v v0x1bfe030_0, 8, 1;
    %jmp T_1;
    .thread T_1;
    .scope S_0x1beaf20;
T_2 ;
    %set/v v0x1bfdf60_0, 0, 32;
    %set/v v0x1bfe030_0, 0, 1;
    %movi 8, 224, 9;
T_2.0 %cmp/s 0, 8, 9;
    %jmp/0xz T_2.1, 5;
    %add 8, 1, 9;
    %delay 1, 0;
    %load/v 17, v0x1bfdf60_0, 32;
    %mov 49, 0, 1;
    %addi 17, 1, 33;
    %set/v v0x1bfdf60_0, 17, 32;
    %jmp T_2.0;
T_2.1 ;
    %movi 8, 30, 6;
T_2.2 %cmp/s 0, 8, 6;
    %jmp/0xz T_2.3, 5;
    %add 8, 1, 6;
    %delay 1, 0;
    %load/v 14, v0x1bfdf60_0, 32;
    %mov 46, 0, 1;
    %addi 14, 1, 33;
    %set/v v0x1bfdf60_0, 14, 32;
    %vpi_call 2 20 "$display", $time, ": addr = %h, data= %h", v0x1bfdf60_0, v0x1bfe0e0_0;
    %jmp T_2.2;
T_2.3 ;
    %vpi_call 2 22 "$finish";
    %end;
    .thread T_2;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "im_test.v";
    "./../mem/im.v";
